CREATE TABLE prueba
(
    ID_prueba int AUTO_INCREMENT,
    ID_clase int NOT NULL,
    P1 VARCHAR (200),
    R1 VARCHAR (200),
    P2 VARCHAR (200),
    P3 VARCHAR (200),
    P4 VARCHAR (200),
    P5 VARCHAR (200),
    P6 VARCHAR (200),
    P7 VARCHAR (200),
    P8 VARCHAR (200),
    P9 VARCHAR (200),
    P10 VARCHAR (200)
)